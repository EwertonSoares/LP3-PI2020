<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="413.0" prefWidth="712.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdmTablePageController">
   <children>
      <TableView fx:id="tableViewBooks" layoutX="4.0" layoutY="39.0" prefHeight="229.0" prefWidth="711.0">
        <columns>
            <TableColumn fx:id="clnCodBook" prefWidth="67.0" text="Codigo " />
          <TableColumn fx:id="clnBookName" onEditCommit="#getNewBookName" prefWidth="98.0" text="Livro " />
          <TableColumn fx:id="clnGenre" minWidth="0.0" prefWidth="95.0" text="Gênero" />
            <TableColumn fx:id="clnPub" minWidth="0.0" prefWidth="71.0" text="Editora" />
            <TableColumn fx:id="clnAut" minWidth="0.0" prefWidth="111.0" text="Autor" />
            <TableColumn fx:id="clnPrice" minWidth="0.0" onEditCommit="#getNewPrice" prefWidth="55.0" text="Preço" />
            <TableColumn fx:id="clnReaDate" onEditCommit="#getNewReleaseDate" prefWidth="60.0" text="Retirada" />
            <TableColumn fx:id="clnExpectedDate" prefWidth="89.0" text="Retorno &#10;esperado" />
            <TableColumn fx:id="clnRetDate" onEditCommit="#getNewReturnDate" prefWidth="69.0" text="Retorno" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnRemove" layoutX="617.0" layoutY="350.0" mnemonicParsing="false" onAction="#removeBook" text="Remover" />
      <Button fx:id="btnUpdate" layoutX="534.0" layoutY="350.0" mnemonicParsing="false" onAction="#updateBook" text="Atualizar" />
      <Button fx:id="btnClose" layoutX="656.0" layoutY="2.0" mnemonicParsing="false" onAction="#closeAdminMainPage" text="Fechar" />
      <ComboBox fx:id="cbGenre" layoutX="203.0" layoutY="291.0" prefHeight="25.0" prefWidth="96.0" />
      <ComboBox fx:id="cbPub" layoutX="309.0" layoutY="291.0" prefHeight="25.0" prefWidth="129.0" />
      <ComboBox fx:id="cbAlt" layoutX="445.0" layoutY="291.0" prefWidth="150.0" />
      <TextField fx:id="txtBook" layoutX="22.0" layoutY="291.0" />
      <TextField fx:id="txtPrice" layoutX="601.0" layoutY="291.0" prefHeight="25.0" prefWidth="110.0" />
      <Button fx:id="btnInsert" layoutX="458.0" layoutY="350.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Inserir" />
   </children>
</AnchorPane>
